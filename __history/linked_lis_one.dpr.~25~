program linked_lis_one;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils,
  Unit1 in 'C:\Users\admin\Documents\Embarcadero\Studio\Projects\Unit1.pas';


var Darr:Tarray;
i, max, sum, min, len:integer;
begin
SetLength(Darr, 10);
Len:= 10;
ReadArray(Darr, len);

sum:= GetArrSum(Darr, len);
writeln('Sum of elements of the array = ', sum);

min:= GetArrMinEl()
writeln('The minimum of array = ', min);

WriteArray(Darr, Len);
readln;

end.
